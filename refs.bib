journal = {Molecular Cell},
year = {2015},
volume = {59},
number = {3},
pages = {449--461},
month = aug,
affiliation = {Department of Molecular Biophysics and Biochemistry, Howard Hughes Medical Institute, Boyer Center for Molecular Medicine, Yale~University School of Medicine, 295 Congress Avenue, New Haven, CT 06536, USA.},
