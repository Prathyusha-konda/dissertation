author = {Pandit, Shatakshi and Zhou, Yu and Shiue, Lily and Coutinho-Mansfield, Gabriela and Li, Hairi and Qiu, Jinsong and Huang, Jie and Yeo, Gene W and Ares Jr, Manuel and Fu, Xiang-Dong},
title = {{Genome-wide Analysis Reveals SR Protein Cooperation and Competition in Regulated Splicing.}},
journal = {Molecular Cell},
year = {2013},
volume = {50},
number = {2},
pages = {223--235},
month = apr,
affiliation = {Department of Cellular and Molecular Medicine and Institute for Genomic Medicine, University of California, San Diego, La Jolla, CA~92093-0651, USA.},
doi = {10.1016/j.molcel.2013.03.001},
pmid = {23562324},
pmcid = {PMC3640356},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-06-01T18:36:23GMT},
date-modified = {2015-12-22T06:06:04GMT},
abstract = {SR proteins are well-characterized RNA binding proteins that promote exon inclusion by binding to exonic splicing enhancers (ESEs). However, it has been unclear whether regulatory rules deduced on model genes apply generally to activities of SR proteins in the cell. Here, we report global analyses of two prototypical SR proteins, SRSF1 (SF2/ASF) and SRSF2 (SC35), using splicing-sensitive arrays and CLIP-seq on mouse embryo fibroblasts (MEFs). Unexpectedly, we find that these SR proteins promote both inclusion and skipping of exons in~vivo, but their binding patterns do not explain such opposite responses. Further analyses reveal that loss of one SR protein is accompanied by coordinated loss or compensatory gain in the interaction of other SR proteins at the affected exons. Therefore, specific effects on regulated splicing by one SR protein actually depend on a complex set of relationships with multiple other SR proteins in mammalian genomes.
                Copyright {\textcopyright} 2013 Elsevier Inc. All rights reserved.},
url = {http://dx.doi.org/10.1016/j.molcel.2013.03.001},
local-url = {file://localhost/Users/olga/Documents/Library.papers3/Articles/2013/Pandit/Molecular%20Cell%202013%20Pandit.pdf},
file = {{Molecular Cell 2013 Pandit.pdf:/Users/olga/Documents/Library.papers3/Articles/2013/Pandit/Molecular Cell 2013 Pandit.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.molcel.2013.03.001}}
}

