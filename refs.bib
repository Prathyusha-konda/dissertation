file = {{BMC Bioinformatics 2009 Xi.pdf:/Users/olga/Documents/Library.papers3/Articles/2009/Xi/BMC Bioinformatics 2009 Xi.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1186/1471-2105-10-232}}
