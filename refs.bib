@article{Magwene:2003wa,
author = {Magwene, Paul M and Lizardi, Paul and Kim, Junhyong},
title = {{Reconstructing the temporal ordering of biological samples using microarray data.}},
journal = {Bioinformatics},
year = {2003},
volume = {19},
number = {7},
pages = {842--850},
month = may,
affiliation = {Department of Ecology and Evolutionary Biology, Yale University School of Medicine, New Haven, CT, USA.},
pmid = {12724294},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-06-04T21:33:52GMT},
date-modified = {2017-02-09T01:00:25GMT},
abstract = {MOTIVATION:Accurate time series for biological processes are difficult to estimate due to problems of synchronization, temporal sampling and rate heterogeneity. Methods are needed that can utilize multi-dimensional data, such as those resulting from DNA microarray experiments, in order to reconstruct time series from unordered or poorly ordered sets of observations.

RESULTS:We present a set of algorithms for estimating temporal orderings from unordered sets of sample elements. The techniques we describe are based on modifications of a minimum-spanning tree calculated from a weighted, undirected graph. We demonstrate the efficacy of our approach by applying these techniques to an artificial data set as well as several gene expression data sets derived from DNA microarray experiments. In addition to estimating orderings, the techniques we describe also provide useful heuristics for assessing relevant properties of sample datasets such as noise and sampling intensity, and we show how a data structure called a PQ-tree can be used to represent uncertainty in a reconstructed ordering.

AVAILABILITY:Academic implementations of the ordering algorithms are available as source code (in the programming language Python) on our web site, along with documentation on their use. The artificial 'jelly roll' data set upon which the algorithm was tested is also available from this web site. The publicly available gene expression data may be found at http://genome-www.stanford.edu/cellcycle/ and http://caulobacter.stanford.edu/CellCycle/.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=12724294&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/olga/Documents/Library.papers3/Articles/2003/Magwene/Bioinformatics%202003%20Magwene.pdf},
file = {{Bioinformatics 2003 Magwene.pdf:/Users/olga/Documents/Library.papers3/Articles/2003/Magwene/Bioinformatics 2003 Magwene.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/935D7329-00F3-4ADD-AD6B-47CE5AB3789F}}
}

