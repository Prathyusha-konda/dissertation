@article{Li:2007js,
author = {Li, Ju and Lykotrafitis, George and Dao, Ming and Suresh, Subra},
title = {{Cytoskeletal dynamics of human erythrocyte.}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
