@article{Mawuenyega:2010go,
author = {Mawuenyega, K G and Sigurdson, W and Ovod, V and Munsell, L and Kasten, T and Morris, J C and Yarasheski, K E and Bateman, R J},
title = {{Decreased Clearance of CNS ~-Amyloid in Alzheimer's Disease}},
journal = {Science},
year = {2010},
volume = {330},
number = {6012},
pages = {1774--1774},
month = dec,
doi = {10.1126/science.1197623},
pmid = {21148344},
pmcid = {PMC3073454},
language = {English},
rating = {0},
date-added = {2013-05-29T06:58:24GMT},
date-modified = {2015-12-22T06:05:39GMT},
abstract = {Alzheimer's disease is hypothesized to be caused by an imbalance between $\beta$-amyloid (A$\beta$) production and clearance that leads to A$\beta$ accumulation in the central nervous system (CNS). A$\beta$ production and clearance are key targets in the development of disease-modifying therapeutic agents for Alzheimer's disease. However, there has not been direct evidence of altered A$\beta$ production or clearance in Alzheimer's disease. By using metabolic labeling, we measured A$\beta$42 and A$\beta$40 production and clearance rates in the CNS of participants with Alzheimer's disease and cognitively normal controls. Clearance rates for both A$\beta$42 and A$\beta$40 were impaired in Alzheimer's disease compared with controls. On average, there were no differences in A$\beta$40 or A$\beta$42 production rates. Thus, the common late-onset form of Alzheimer's disease is characterized by an overall impairment in A$\beta$ clearance.},
url = {http://www.sciencemag.org/cgi/doi/10.1126/science.1197623},
local-url = {file://localhost/Users/olga/Documents/Library.papers3/Articles/2010/Mawuenyega/Science%202010%20Mawuenyega.pdf},
file = {{Science 2010 Mawuenyega.pdf:/Users/olga/Documents/Library.papers3/Articles/2010/Mawuenyega/Science 2010 Mawuenyega.pdf:application/pdf}},
uri = {\url{papers3://publication/livfe/id/64399}}
}

