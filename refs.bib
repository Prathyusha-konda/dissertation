@article{McCormack:2011fc,
author = {McCormack, Mark and Alfirevic, Ana and Bourgeois, Stephane and Farrell, John J and Kasperavi{\v c}i{\=u}t{\.{e}}, Dalia and Carrington, Mary and Sills, Graeme J and Marson, Tony and Jia, Xiaoming and de Bakker, Paul I W and Chinthapalli, Krishna and Molokhia, Mariam and Johnson, Michael R and O'Connor, Gerard D and Chaila, Elijah and Alhusaini, Saud and Shianna, Kevin V and Radtke, Rodney A and Heinzen, Erin L and Walley, Nicole and Pandolfo, Massimo and Pichler, Werner and Park, B Kevin and Depondt, Chantal and Sisodiya, Sanjay M and Goldstein, David B and Deloukas, Panos and Delanty, Norman and Cavalleri, Gianpiero L and Pirmohamed, Munir},
title = {{HLA-A*3101 and carbamazepine-induced hypersensitivity reactions in Europeans.}},
journal = {New England Journal of Medicine},
year = {2011},
volume = {364},
number = {12},
pages = {1134--1143},
month = mar,
affiliation = {Molecular and Cellular Therapeutics, the Royal College of Surgeons in Ireland, Dublin, Ireland.},
doi = {10.1056/NEJMoa1013297},
pmid = {21428769},
pmcid = {PMC3113609},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-12-11T07:10:21GMT},
date-modified = {2013-02-05T04:33:46GMT},
abstract = {BACKGROUND:Carbamazepine causes various forms of hypersensitivity reactions, ranging from maculopapular exanthema to severe blistering reactions. The HLA-B*1502 allele has been shown to be strongly correlated with carbamazepine-induced Stevens-Johnson syndrome and toxic epidermal necrolysis (SJS-TEN) in the Han Chinese and other Asian populations but not in European populations.

METHODS:We performed a genomewide association study of samples obtained from 22 subjects with carbamazepine-induced hypersensitivity syndrome, 43 subjects with carbamazepine-induced maculopapular exanthema, and 3987 control subjects, all of European descent. We tested for an association between disease and HLA alleles through proxy single-nucleotide polymorphisms and imputation, confirming associations by high-resolution sequence-based HLA typing. We replicated the associations in samples from 145 subjects with carbamazepine-induced hypersensitivity reactions.

RESULTS:The HLA-A*3101 allele, which has a prevalence of 2 to 5% in Northern European populations, was significantly associated with the hypersensitivity syndrome (P=3.5{\texttimes}10(-8)). An independent genomewide association study of samples from subjects with maculopapular exanthema also showed an association with the HLA-A*3101 allele (P=1.1{\texttimes}10(-6)). Follow-up genotyping confirmed the variant as a risk factor for the hypersensitivity syndrome (odds ratio, 12.41; 95% confidence interval [CI], 1.27 to 121.03), maculopapular exanthema (odds ratio, 8.33; 95% CI, 3.59 to 19.36), and SJS-TEN (odds ratio, 25.93; 95% CI, 4.93 to 116.18).

CONCLUSIONS:The presence of the HLA-A*3101 allele was associated with carbamazepine-induced hypersensitivity reactions among subjects of Northern European ancestry. The presence of the allele increased the risk from 5.0% to 26.0%, whereas its absence reduced the risk from 5.0% to 3.8%. (Funded by the U.K. Department of Health and others.).},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=21428769&retmode=ref&cmd=prlinks},
uri = {\url{papers3://publication/doi/10.1056/NEJMoa1013297}}
}

