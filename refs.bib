@article{Thompson:1994tz,
author = {Thompson, J D and Higgins, D G and Gibson, T J},
title = {{CLUSTAL W: improving the sensitivity of progressive multiple sequence alignment through sequence weighting, position-specific gap penalties and weight matrix choice.}},
journal = {Nucleic Acids Research},
year = {1994},
volume = {22},
number = {22},
pages = {4673--4680},
month = nov,
affiliation = {European Molecular Biology Laboratory, Heidelberg, Germany.},
pmid = {7984417},
pmcid = {PMC308517},
language = {English},
rating = {0},
date-added = {2012-10-22T17:45:10GMT},
date-modified = {2015-12-22T06:05:27GMT},
abstract = {The sensitivity of the commonly used progressive multiple sequence alignment method has been greatly improved for the alignment of divergent protein sequences. Firstly, individual weights are assigned to each sequence in a partial alignment in order to down-weight near-duplicate sequences and up-weight the most divergent ones. Secondly, amino acid substitution matrices are varied at different alignment stages according to the divergence of the sequences to be aligned. Thirdly, residue-specific gap penalties and locally reduced gap penalties in hydrophilic regions encourage new gaps in potential loop regions rather than regular secondary structure. Fourthly, positions in early alignments where gaps have been opened receive locally reduced gap penalties to encourage the opening up of new gaps at these positions. These modifications are incorporated into a new program, CLUSTAL W which is freely available.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=7984417&retmode=ref&cmd=prlinks},
