@article{Lalo:1993tq,
author = {Lalo, D and Carles, C and Sentenac, A and Thuriaux, P},
title = {{Interactions between three common subunits of yeast RNA polymerases I and III.}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {1993},
volume = {90},
number = {12},
pages = {5524--5528},
month = jun,
affiliation = {D{\'e}partement de Biologie Cellulaire et Mol{\'e}culaire, Centre d'Etudes de Saclay, Gif sur Yvette, France.},
pmid = {8516295},
