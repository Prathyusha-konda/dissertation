@article{Sardiello:2009bk,
author = {Sardiello, Marco and Palmieri, Michela and di Ronza, Alberto and Medina, Diego Luis and Valenza, Marta and Gennarino, Vincenzo Alessandro and Di Malta, Chiara and Donaudy, Francesca and Embrione, Valerio and Polishchuk, Roman S and Banfi, Sandro and Parenti, Giancarlo and Cattaneo, Elena and Ballabio, Andrea},
