@article{Overington:1990fj,
author = {Overington, J and Johnson, M S and Sali, A and Blundell, T L},
title = {{Tertiary structural constraints on protein evolutionary diversity: templates, key residues and structure prediction.}},
journal = {Proceedings. Biological sciences / The Royal Society},
year = {1990},
volume = {241},
number = {1301},
pages = {132--145},
month = aug,
affiliation = {Department of Crystallography, Birkbeck College, University of London, U.K.},
