@article{Mehta:2009ca,
author = {Mehta, Timir Y and Prajapati, Laxman M and Mittal, Bharti and Joshi, Chaitanya G and Sheth, Jayesh J and Patel, Dinesh B and Dave, Dinkar M and Goyal, Ramesh K},
title = {{Association of HLA-B*1502 allele and carbamazepine-induced Stevens-Johnson syndrome among Indians.}},
journal = {Indian journal of dermatology, venereology and leprology},
year = {2009},
volume = {75},
number = {6},
pages = {579--582},
month = nov,
affiliation = {Dr. Rasiklal Shah Sarvajanik Hospital, Modasa 383315, India. samarpan_research@yahoo.co.in},
doi = {10.4103/0378-6323.57718},
pmid = {19915237},
language = {English},
rating = {0},
date-added = {2012-12-11T07:05:47GMT},
date-modified = {2013-02-05T04:33:49GMT},
abstract = {BACKGROUND:Stevens-Johnson Syndrome (SJS) and toxic epidermal necrolysis are severe cutaneous reactions caused by certain drugs, including antiepileptic carbamazepine. A strong association has been reported between human leucocyte antigen (HLA)-B*1502 and carbamazepine-induced SJS in Han Chinese patients. European studies suggested that HLA-B*1502 is not a universal marker but is ethnicity-specific for Asians.

AIM:To study the association between HLA-B*1502 and carbamazepine-induced SJS in Indian patients.

METHODS:Eight individuals who fulfilled the diagnostic criteria of SJS induced by carbamazepine were identified and HLA-B molecular typing was performed. HLA-B genotyping was carried out by polymerase chain reaction using sequence-specific primers.

RESULTS:Out of eight patients studied for genotype, six patients were found to have the HLA-B*1502 allele.

CONCLUSION:This study suggests an association between HLA-B*1502 and carbamazepine-induced SJS in Indian patients.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=19915237&retmode=ref&cmd=prlinks},
uri = {\url{papers3://publication/doi/10.4103/0378-6323.57718}}
}

