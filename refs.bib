@article{Anonymous:cfVBNi6Y,
author = {Kolkman, J A and Stemmer, W P},
title = {{Directed evolution of proteins by exon shuffling.}},
journal = {Nat Biotech},
year = {2001},
volume = {19},
number = {5},
pages = {423--428},
month = may,
affiliation = {Maxygen Inc., 515 Galveston Drive, Redwood City, CA 94063, USA.},
doi = {10.1038/88084},
pmid = {11329010},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-12-23T17:22:27GMT},
date-modified = {2015-12-22T06:05:27GMT},
abstract = {Evolution of eukaryotes is mediated by sexual recombination of parental genomes. Crossovers occur in random, but homologous, positions at a frequency that depends on DNA length. As exons occupy only 1% of the human genome and introns about 24%, by far most of the crossovers occur between exons, rather than inside. The natural process of creating new combinations of exons by intronic recombination is called exon shuffling. Our group is developing in vitro formats for exon shuffling and applying these to the directed evolution of proteins. Based on the splice frame junctions, nine classes of exons and three classes of introns can be distinguished. Splice frame diagrams of natural genes show how the splice frame rules govern exon shuffling. Here, we review various approaches to constructing libraries of exon-shuffled genes. For example, exon shuffling of human pharmaceutical proteins can generate libraries in which all of the sequences are fully human, without the point mutations that raise concerns about immunogenicity.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=11329010&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/olga/Documents/Library.papers3/Articles/2001/Kolkman/Nat%20Biotech%202001%20Kolkman.pdf},
file = {{Nat Biotech 2001 Kolkman.pdf:/Users/olga/Documents/Library.papers3/Articles/2001/Kolkman/Nat Biotech 2001 Kolkman.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1038/88084}}
}

