@article{Eng:2009gp,
author = {Eng, Kevin H and Bravo, H{\'e}ctor Corrada and Kele{\c s}, S{\"u}nd{\"u}z},
title = {{A phylogenetic mixture model for the evolution of gene expression.}},
journal = {Molecular biology and evolution},
year = {2009},
volume = {26},
number = {10},
pages = {2363--2372},
month = oct,
affiliation = {Department of Statistics, University of Wisconsin-Madison, Madison, WI, USA.},
doi = {10.1093/molbev/msp149},
pmid = {19602540},
pmcid = {PMC2738779},
language = {English},
rating = {0},
date-added = {2012-10-13T00:48:52GMT},
date-modified = {2015-12-22T06:05:35GMT},
abstract = {Microarray platforms are used increasingly to make comparative inferences through genome-wide surveys of gene expression. Although recent studies focus on describing the evidence for natural selection using estimates of the within- and between-taxa mutational variances, these methods do not explicitly or flexibly account for predicted nonindependence due to phylogenetic associations between measurements. In the interest of parsing the effects of selection: we introduce a mixture model for the comparative analysis of variation in gene expression across multiple taxa. This class of models isolates the phylogenetic signal from the nonphylogenetic and the heritable signal from the nonheritable while measuring the proper amount of correction. As a result, the mixture model resolves outstanding differences between existing models, relates different ways to estimate the across taxa variance, and induces a likelihood ratio test for selection. We investigate by simulation and application the feasibility and utility of estimation of the required parameters and the power of the proposed test. We illustrate analysis under this mixture model with a gene duplication family data set.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=19602540&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/olga/Documents/Library.papers3/Articles/2009/Eng/Mol.%20Biol.%20Evol.%202009%20Eng.pdf},
file = {{Mol. Biol. Evol. 2009 Eng.pdf:/Users/olga/Documents/Library.papers3/Articles/2009/Eng/Mol. Biol. Evol. 2009 Eng.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1093/molbev/msp149}}
