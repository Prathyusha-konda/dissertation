file = {{eLife 2014 Katz.pdf:/Users/olga/Documents/Library.papers3/Articles/2014/Katz/eLife 2014 Katz.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.7554/eLife.03915}}
}

