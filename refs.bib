@article{BinWu:2016gt,
author = {Bin Wu and Eliscovich, Carolina and Yoon, Young J and Singer, Robert H},
title = {{Translation dynamics of single mRNAs in live cells and neurons.}},
journal = {Science},
year = {2016},
volume = {352},
number = {6292},
pages = {1430--1435},
month = jun,
publisher = {American Association for the Advancement of Science},
affiliation = {Department of Anatomy and Structural Biology, Albert Einstein College of Medicine, Bronx, NY 10461, USA. Gruss-Lipper Biophotonics Center, Albert Einstein College of Medicine, Bronx, NY 10461, USA.},
doi = {10.1126/science.aaf1084},
pmid = {27313041},
pmcid = {PMC4939616},
read = {Yes},
rating = {0},
date-added = {2016-05-05T20:59:27GMT},
date-modified = {2017-02-09T01:00:08GMT},
abstract = {Translation is the fundamental biological process converting mRNA information into proteins. Single-molecule imaging in live cells has illuminated the dynamics of RNA transcription; however, it is not yet applicable to translation. Here, we report single-molecule imaging of nascent peptides (SINAPS) to assess translation in live cells. The approach provides direct readout of initiation, elongation, and location of translation. We show that mRNAs coding for endoplasmic reticulum (ER) proteins are translated when they encounter the ER membrane. Single-molecule fluorescence recovery after photobleaching provides direct measurement of elongation speed (5 amino acids per second). In primary neurons, mRNAs are translated in proximal dendrites but repressed in distal dendrites and display "bursting" translation. This technology provides a tool with which to address the spatiotemporal translation mechanism of single mRNAs in living cells. Copyright {\textcopyright} 2016, American Association for the Advancement of Science.},
url = {http://www.sciencemag.org/cgi/doi/10.1126/science.aaf1084},
local-url = {file://localhost/Users/olga/Documents/Library.papers3/Articles/2016/Bin%20Wu/Science%202016%20Bin%20Wu.pdf},
file = {{Science 2016 Bin Wu.pdf:/Users/olga/Documents/Library.papers3/Articles/2016/Bin Wu/Science 2016 Bin Wu.pdf:application/pdf;Science 2016 Bin Wu.pdf:/Users/olga/Documents/Library.papers3/Articles/2016/Bin Wu/Science 2016 Bin Wu.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1126/science.aaf1084}}
