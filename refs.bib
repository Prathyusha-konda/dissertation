@article{Revil:2010fo,
author = {Revil, Timoth{\'e}e and Gaffney, Daniel and Dias, Christel and Majewski, Jacek and Jerome-Majewska, Loydie A},
title = {{Alternative splicing is frequent during early embryonic development in mouse.}},
journal = {BMC Genomics},
year = {2010},
volume = {11},
pages = {399},
affiliation = {Department of Human Genetics, McGill University, Montreal, Quebec, Canada. timothee.revil@mcgill.ca},
doi = {10.1186/1471-2164-11-399},
pmid = {20573213},
pmcid = {PMC2898759},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-05-10T00:21:13GMT},
date-modified = {2017-02-09T01:00:23GMT},
abstract = {BACKGROUND:Alternative splicing is known to increase the complexity of mammalian transcriptomes since nearly all mammalian genes express multiple pre-mRNA isoforms. However, our knowledge of the extent and function of alternative splicing in early embryonic development is based mainly on a few isolated examples. High throughput technologies now allow us to study genome-wide alternative splicing during mouse development.

RESULTS:A genome-wide analysis of alternative isoform expression in embryonic day 8.5, 9.5 and 11.5 mouse embryos and placenta was carried out using a splicing-sensitive exon microarray. We show that alternative splicing and isoform expression is frequent across developmental stages and tissues, and is comparable in frequency to the variation in whole-transcript expression. The genes that are alternatively spliced across our samples are disproportionately involved in important developmental processes. Finally, we find that a number of RNA binding proteins, including putative splicing factors, are differentially expressed and spliced across our samples suggesting that such proteins may be involved in regulating tissue and temporal variation in isoform expression. Using an example of a well characterized splicing factor, Fox2, we demonstrate that changes in Fox2 expression levels can be used to predict changes in inclusion levels of alternative exons that are flanked by Fox2 binding sites.

CONCLUSIONS:We propose that alternative splicing is an important developmental regulatory mechanism. We further propose that gene expression should routinely be monitored at both the whole transcript and the isoform level in developmental studies.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20573213&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/olga/Documents/Library.papers3/Articles/2010/Revil/BMC%20Genomics%202010%20Revil.pdf},
file = {{BMC Genomics 2010 Revil.pdf:/Users/olga/Documents/Library.papers3/Articles/2010/Revil/BMC Genomics 2010 Revil.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1186/1471-2164-11-399}}
}

