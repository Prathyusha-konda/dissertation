journal = {Trends in genetics : TIG},
year = {2005},
volume = {21},
number = {2},
pages = {73--77},
month = feb,
affiliation = {CH Best Institute, University of Toronto, 112 College Street, Toronto, Ontario, Canada M5G 1L6.},
