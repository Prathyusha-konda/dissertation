pages = {31},
affiliation = {Department of Genetics, MD Anderson Cancer Center, Houston, TX 77030, USA. NNavin@mdanderson.org.},
doi = {10.1186/gm247},
pmid = {21631906},
pmcid = {PMC3219072},
language = {English},
rating = {0},
date-added = {2012-05-22T18:03:08GMT},
date-modified = {2015-12-22T06:05:45GMT},
abstract = {Advances in whole genome amplification and next-generation sequencing methods have enabled genomic analyses of single cells, and these techniques are now beginning to be used to detect genomic lesions in individual cancer cells. Previous approaches have been unable to resolve genomic differences in complex mixtures of cells, such as heterogeneous tumors, despite the importance of characterizing such tumors for cancer treatment. Sequencing of single cells is likely to improve several aspects of medicine, including the early detection of rare tumor cells, monitoring of circulating tumor cells (CTCs), measuring intratumor heterogeneity, and guiding chemotherapy. In this review we discuss the challenges and technical aspects of single-cell sequencing, with a strong focus on genomic copy number, and discuss how this information can be used to diagnose and treat cancer patients.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=21631906&retmode=ref&cmd=prlinks},
