@article{Kaniwa:2008fi,
author = {Kaniwa, Nahoko and Saito, Yoshiro and Aihara, Michiko and Matsunaga, Kayoko and Tohkin, Masahiro and Kurose, Kouichi and Sawada, Jun-ichi and Furuya, Hirokazu and Takahashi, Yukitoshi and Muramatsu, Masaaki and Kinoshita, Shigeru and Abe, Masamichi and Ikeda, Hiroko and Kashiwagi, Mariko and Song, Yixuan and Ueta, Mayumi and Sotozono, Chie and Ikezawa, Zenro and Hasegawa, Ryuichi and {JSAR research group}},
title = {{HLA-B locus in Japanese patients with anti-epileptics and allopurinol-related Stevens-Johnson syndrome and toxic epidermal necrolysis.}},
journal = {Pharmacogenomics},
year = {2008},
volume = {9},
number = {11},
pages = {1617--1622},
month = nov,
affiliation = {Division of Medicinal Safety Science, National Institute of Health Sciences, 1-18-11 Kamiyoga, Setagaya-ku, Tokyo. nkaniwa@nihs.go.jp},
doi = {10.2217/14622416.9.11.1617},
pmid = {19018717},
language = {English},
rating = {0},
date-added = {2012-12-11T07:08:17GMT},
date-modified = {2013-02-05T04:33:40GMT},
abstract = {INTRODUCTION:Stevens-Johnson syndrome (SJS) and toxic epidermal necrolysis (TEN) are rare but life-threatening severe cutaneous adverse reactions. Recently, strong associations of HLA-B*1502 and HLA-B*5801 with carbamazepine- and allopurinol-induced severe cutaneous adverse reactions were found in Han Chinese patients, respectively, but ethnic differences in the associations have been reported. The objective of this study is to clarify the involvement of HLA-B*1502 and HLA-B*5801 in Japanese SJS/TEN patients.

METHODS:HLA-B genotyping was performed on 58 Japanese SJS/TEN patients between July 2006 and April 2008 from multicenters in Japan.

RESULTS:There were no HLA-B*1502 carriers among 58 SJS/TEN patients. This patient group included seven carbamazepine-related and 11 aromatic anti-epileptic agent-related SJS/TEN patients. In addition, there were five HLA-B*5801 carriers, which included four allopurinol-related SJS/TEN patients.

CONCLUSION:While HLA-B*1502 is unlikely to be associated with carbamazepine-related or aromatic anti-epileptic agent-related SJS/TEN, HLA-B*5801 was significantly associated with allopurinol-related SJS/TEN in Japanese.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=19018717&retmode=ref&cmd=prlinks},
uri = {\url{papers3://publication/doi/10.2217/14622416.9.11.1617}}
}

