@article{Harbison:2004bp,
author = {Harbison, Christopher T and Gordon, D Benjamin and Lee, Tong Ihn and Rinaldi, Nicola J and Macisaac, Kenzie D and Danford, Timothy W and Hannett, Nancy M and Tagne, Jean-Bosco and Reynolds, David B and Yoo, Jane and Jennings, Ezra G and Zeitlinger, Julia and Pokholok, Dmitry K and Kellis, Manolis and Rolfe, P Alex and Takusagawa, Ken T and Lander, Eric S and Gifford, David K and Fraenkel, Ernest and Young, Richard A},
