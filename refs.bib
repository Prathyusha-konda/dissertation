@article{Damle:1999ur,
author = {Damle, R N and Wasil, T and Fais, F and Ghiotto, F and Valetto, A and Allen, S L and Buchbinder, A and Budman, D and Dittmar, K and Kolitz, J and Lichtman, S M and Schulman, P and Vinciguerra, V P and Rai, K R and Ferrarini, M and Chiorazzi, N},
title = {{Ig V gene mutation status and CD38 expression as novel prognostic indicators in chronic lymphocytic leukemia.}},
journal = {Blood},
year = {1999},
volume = {94},
number = {6},
pages = {1840--1847},
month = sep,
affiliation = {Department of Medicine, North Shore University Hospital, Manhasset, NY, USA.},
pmid = {10477712},
language = {English},
rating = {0},
date-added = {2012-10-13T19:22:15GMT},
date-modified = {2015-12-22T06:05:27GMT},
abstract = {Cellular immunophenotypic studies were performed on a cohort of randomly selected IgM(+) B-chronic lymphocytic leukemia (B-CLL) cases for which Ig V(H) and V(L) gene sequences were available. The cases were categorized based on V gene mutation status and CD38 expression and analyzed for treatment history and survival. The B-CLL cases could be divided into 2 groups. Those patients with unmutated V genes displayed higher percentages of CD38(+) B-CLL cells (>/=30%) than those with mutated V genes that had lower percentages of CD38(+) cells (<30%). Patients in both the unmutated and the >/=30% CD38(+) groups responded poorly to continuous multiregimen chemotherapy (including fludarabine) and had shorter survival. In contrast, the mutated and the <30% CD38(+) groups required minimal or no chemotherapy and had prolonged survival. These observations were true also for those patients who stratified to the Rai intermediate risk category. In the mutated and the <30% CD38(+) groups, males and females were virtually equally distributed, whereas in the unmutated and the >/=30% CD38(+) groups, a marked male predominance was found. Thus, Ig V gene mutation status and the percentages of CD38(+) B-CLL cells appear to be accurate predictors of clinical outcome in B-CLL patients. These parameters, especially CD38 expression that can be analyzed conveniently in most clinical laboratories, should be valuable adjuncts to the present staging systems for predicting the clinical course in individual B-CLL cases. Future evaluations of new therapeutic strategies and drugs should take into account the different natural histories of patients categorized in these manners.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=10477712&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/olga/Documents/Library.papers3/Articles/1999/Damle/Blood%201999%20Damle.pdf},
file = {{Blood 1999 Damle.pdf:/Users/olga/Documents/Library.papers3/Articles/1999/Damle/Blood 1999 Damle.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/93E2354C-FA20-4D4B-98F1-D2036F489889}}
}

