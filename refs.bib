@article{Coady:2015jia,
author = {Coady, Tristan H and Manley, James L},
title = {{ALS mutations in TLS/FUS disrupt target gene expression}},
journal = {Genes {\&} Development},
year = {2015},
pages = {1--12},
month = aug,
publisher = {Cold Spring Harbor Lab},
affiliation = {Department of Biological Sciences, Columbia University, New York, New York 10027, USA.},
doi = {10.1101/gad.267286.115},
pmid = {26251528},
language = {English},
rating = {0},
date-added = {2015-08-07T00:06:37GMT},
date-modified = {2017-02-09T01:00:20GMT},
abstract = {Amyotrophic lateral sclerosis (ALS) is caused by mutations in a number of genes, including the gene encoding the RNA/DNA-binding protein translocated in liposarcoma or fused in sarcoma (TLS/FUS or FUS). Previously, we identified a number of FUS target genes, among them MECP2. To investigate how ALS mutations in FUS might impact target gene expression, we examined the effects of several FUS derivatives harboring ALS mutations, such as R521C (FUS(C)), on MECP2 expression in transfected human U87 cells. Strikingly, FUS(C) and other mutants not only altered MECP2 alternative splicing but also markedly increased mRNA abundance, which we show resulted from sharply elevated stability. Paradoxically, however, MeCP2 protein levels were significantly reduced in cells expressing ALS mutant derivatives. Providing a parsimonious explanation for these results, biochemical fractionation and in vivo localization studies revealed that MECP2 mRNA colocalized with cytoplasmic FUS(C) in insoluble aggregates, which are characteristic of ALS mutant proteins. Together, our results establish that ALS mutations in FUS can strongly impact target gene expression, reflecting a dominant effect of FUS-containing aggregates.},
url = {http://genesdev.cshlp.org/lookup/doi/10.1101/gad.267286.115},
local-url = {file://localhost/Users/olga/Documents/Library.papers3/Articles/2015/Coady/Genes%20&%20Development%202015%20Coady-2.pdf},
file = {{Genes & Development 2015 Coady-2.pdf:/Users/olga/Documents/Library.papers3/Articles/2015/Coady/Genes & Development 2015 Coady-2.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1101/gad.267286.115}}
}

