@article{Shi:2009dz,
author = {Shi, Yanxin and Klutstein, Michael and Simon, Itamar and Mitchell, Tom and Bar-Joseph, Ziv},
title = {{A Combined Expression-Interaction Model for Inferring the Temporal Activity of Transcription Factors}},
journal = {Journal of Computational Biology},
year = {2009},
volume = {16},
number = {8},
pages = {1035--1049},
month = aug,
affiliation = {Machine Learning Department, School of Computer Science, Carnegie Mellon University , Pittsburgh, PA 15213, USA.},
doi = {10.1089/cmb.2009.0024},
language = {English},
rating = {0},
date-added = {2012-07-22T19:59:00GMT},
date-modified = {2015-12-22T06:05:36GMT},
abstract = {Methods suggested for reconstructing regulatory networks can be divided into two sets based on how the activity level of transcription factors (TFs) is inferred. The first group of methods relies on the expression levels of TFs, assuming that the activity of a TF is highly correlated with its mRNA abundance. The second treats the activity level as unobserved and infers it from the expression of the genes that the TF regulates. While both types of methods were successfully applied, each suffers from drawbacks that limit their accuracy. For the first set, the assumption that mRNA levels are correlated with activity is violated for many TFs due to post-transcriptional modifications. For the second, the expression level of a TF which might be informative is completely ignored. Here we present the post-transcriptional modification model (PTMM) that, unlike previous methods, utilizes both sources of data concurrently. Our method uses a switching model to determine whether a TF is transcriptionally or post-transcriptionally regulated. This model is combined with a factorial HMM to reconstruct the interactions in a dynamic regulatory network. Using simulated and real data, we show that PTMM outperforms the other two approaches discussed above. Using real data, we also show that PTMM can recover meaningful TF activity levels and identify post-transcriptionally modified TFs, many of which are supported by other sources. Supporting website: www.sb.cs.cmu.edu/PTMM/PTMM.html.},
url = {http://www.liebertonline.com/doi/abs/10.1089/cmb.2009.0024},
local-url = {file://localhost/Users/olga/Documents/Library.papers3/Articles/2009/Shi/Journal%20of%20Computational%20Biology%202009%20Shi.pdf},
file = {{Journal of Computational Biology 2009 Shi.pdf:/Users/olga/Documents/Library.papers3/Articles/2009/Shi/Journal of Computational Biology 2009 Shi.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1089/cmb.2009.0024}}
}

