@article{Anonymous:T7jCPdmt,
author = {Li, F and Ryvkin, P and Childress, D M and Valladares, O and Gregory, B D and Wang, L S},
title = {{SAVoR: a server for sequencing annotation and visualization of RNA structures}},
journal = {Nucleic Acids Research},
year = {2012},
volume = {40},
number = {W1},
month = jun,
affiliation = {Genomics and Computational Biology Graduate Group, Perelman School of Medicine, University of Pennsylvania, Penn Genome Frontiers Institute, Perelman School of Medicine, University of Pennsylvania, Department of Biology, Department of Pathology and Laboratory Medicine, Institute on Aging, Perelman School of Medicine, University of Pennsylvania and Penn Center for Bioinformatics, University of Pennsylvania, Philadelphia, PA 19104, USA.},
