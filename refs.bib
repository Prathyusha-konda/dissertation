title = {{Analysis of gene expression in single live neurons.}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {1992},
volume = {89},
number = {7},
pages = {3010--3014},
month = apr,
affiliation = {Department of Pharmacology, University of Pennsylvania Medical School, Philadelphia 19104.},
pmid = {1557406},
